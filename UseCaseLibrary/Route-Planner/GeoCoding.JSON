{
  "swagger": "2.0",
  "info": {
    "description": "API to get GeoCodes and location information for locations in Germany",
    "version": "1.0.0",
    "title": "Geocoding API"
  },
  "host": "api.traveltime.com",
  "basePath": "/v1",
  "schemes": [
    "https"
  ],
  "paths": {
    "/geocode": {
      "get": {
        "summary": "Geocoding Request",
        "description": "Get GeoCodes and location information for locations in Germany",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "query",
            "in": "query",
            "description": "Get GeoCodes and location information for locations in Germany",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "example": "FeatureCollection"
                },
                "attribution": {
                  "type": "string",
                  "example": "https://docs.traveltime.com/api/reference/geocoding-attribution"
                },
                "features": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "type": {
                        "type": "string",
                        "example": "Feature"
                      },
                      "geometry": {
                        "type": "object",
                        "properties": {
                          "type": {
                            "type": "string",
                            "example": "Point"
                          },
                          "coordinates": {
                            "type": "array",
                            "items": {
                              "type": "number"
                            },
                            "example": [9.98569, 53.55243]
                          }
                        }
                      },
                      "properties": {
                        "type": "object",
                        "properties": {
                          "name": {
                            "type": "string",
                            "example": "Axel-Springer-Platz 2, 20355 Hamburg, Germany"
                          },
                          "label": {
                            "type": "string",
                            "example": "Axel-Springer-Platz 2, 20355 Hamburg, Germany"
                          },
                          "category": {
                            "type": "string",
                            "example": "building"
                          },
                          "type": {
                            "type": "string",
                            "example": "residential"
                          },
                          "score": {
                            "type": "number",
                            "example": 0.74
                          },
                          "house_number": {
                            "type": "string",
                            "example": "2"
                          },
                          "street": {
                            "type": "string",
                            "example": "Axel-Springer-Platz"
                          },
                          "county": {
                            "type": "string",
                            "example": "Hamburg"
                          },
                          "macroregion": {
                            "type": "string",
                            "example": "Hamburg"
                          },
                          "city": {
                            "type": "string",
                            "example": "Hamburg"
                          },
                          "district": {
                            "type": "string",
                            "example": "Neustadt"
                          },
                          "country": {
                            "type": "string",
                            "example": "Germany"
                          },
                          "country_code": {
                            "type": "string",
                            "example": "DEU"
                          },
                          "postcode": {
                            "type": "string",
                            "example": "20355"
                          },
                          "features": {
                            "type": "object",
                            "properties": {
                              "public_transport": {
                                "type": "object",
                                "properties": {
                                  "date_start": {
                                    "type": "string",
                                    "format": "date-time",
                                    "example": "2025-04-26T00:00:00+02:00"
                                  },
                                  "date_end": {
                                    "type": "string",
                                    "format": "date-time",
                                    "example": "2025-06-29T00:00:00+02:00"
                                  }
                                }
                              },
                              "cross_country_modes": {
                                "type": "array",
                                "items": {
                                  "type": "string"
                                },
                                "example": []
                              },
                              "fares": {
                                "type": "boolean",
                                "example": false
                              },
                              "postcodes": {
                                "type": "boolean",
                                "example": false
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
